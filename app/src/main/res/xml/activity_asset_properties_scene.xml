<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/asdet_transactionHolder_rcv"
            motion:touchAnchorSide="bottom" />
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/asdet_paymentSystem_img"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="@+id/cardView9"
            android:layout_width="0dp"
            android:layout_height="52dp"
            motion:layout_constraintBottom_toBottomOf="@+id/cardView9"
            android:layout_marginEnd="16dp"
            motion:layout_constraintDimensionRatio="1:1"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/asdet_paymentSystem_img"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="@+id/cardView9"
            android:layout_width="0dp"
            android:layout_height="52dp"
            motion:layout_constraintBottom_toBottomOf="@+id/cardView9"
            android:visibility="visible"
            android:layout_marginEnd="16dp"
            motion:layout_constraintDimensionRatio="1:1" />
        <Constraint
            android:id="@+id/asdet_owner_txt"
            android:elevation="8dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/cardView9"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_assetName_txt"
            motion:layout_constraintStart_toStartOf="@+id/asdet_assetName_txt"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/asdet_owner_txt"
            android:elevation="8dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/cardView9"
            android:visibility="visible"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_assetName_txt"
            motion:layout_constraintStart_toStartOf="@+id/asdet_assetName_txt" />
        <Constraint
            android:id="@+id/asdet_assetName_txt"
            android:elevation="8dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="@+id/cardView9"
            motion:layout_constraintStart_toStartOf="@+id/cardView9"
            android:layout_marginTop="32dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/asdet_assetName_txt"
            android:elevation="8dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:visibility="visible"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="@+id/cardView9"
            motion:layout_constraintStart_toStartOf="@+id/cardView9"
            android:layout_marginTop="32dp" />
        <Constraint
            android:id="@+id/asdet_currency_txt"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="@+id/cardView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/asdet_owner_txt"
            android:layout_marginEnd="32dp"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_assetName_txt"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/asdet_currency_txt"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="@+id/cardView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/asdet_owner_txt"
            android:visibility="visible"
            android:layout_marginEnd="32dp"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_assetName_txt" />
        <Constraint
            android:id="@+id/asdet_balance_txt"
            android:elevation="8dp"
            motion:layout_constraintEnd_toStartOf="@+id/asdet_currency_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/asdet_currency_txt"
            android:layout_marginEnd="8dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/asdet_balance_txt"
            android:elevation="8dp"
            motion:layout_constraintEnd_toStartOf="@+id/asdet_currency_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/asdet_currency_txt"
            android:visibility="visible"
            android:layout_marginEnd="8dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_back_btn"
            android:layout_marginStart="16dp"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintDimensionRatio="8:1"
            android:layout_marginTop="8dp"
            android:id="@+id/cardView9" />
        <Constraint
            android:elevation="8dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="@+id/cardView9"
            motion:layout_constraintStart_toStartOf="@+id/cardView9"
            android:layout_marginTop="8dp"
            android:id="@+id/asdet_assetName_txt"/>
        <Constraint
            android:id="@+id/asdet_owner_txt"
            android:elevation="8dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/cardView9"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_assetName_txt"
            motion:layout_constraintStart_toStartOf="@+id/asdet_assetName_txt"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/asdet_owner_txt"
            android:elevation="8dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/cardView9"
            android:visibility="invisible"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_assetName_txt"
            motion:layout_constraintStart_toStartOf="@+id/asdet_assetName_txt" />
        <Constraint
            android:id="@+id/asdet_paymentSystem_img"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="@+id/cardView9"
            android:layout_width="0dp"
            android:layout_height="52dp"
            motion:layout_constraintBottom_toBottomOf="@+id/cardView9"
            android:layout_marginEnd="16dp"
            motion:layout_constraintDimensionRatio="1:1"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/asdet_paymentSystem_img"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="@+id/cardView9"
            android:layout_width="0dp"
            android:layout_height="52dp"
            motion:layout_constraintBottom_toBottomOf="@+id/cardView9"
            android:visibility="invisible"
            android:layout_marginEnd="16dp"
            motion:layout_constraintDimensionRatio="1:1" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/cardView9"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/cardView9"
            android:id="@+id/asdet_isPrimary_chb" />
        <Constraint
            android:id="@+id/asdet_balance_txt"
            android:elevation="8dp"
            motion:layout_constraintEnd_toStartOf="@+id/asdet_currency_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/asdet_currency_txt"
            android:layout_marginEnd="8dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/asdet_balance_txt"
            android:elevation="8dp"
            motion:layout_constraintEnd_toStartOf="@+id/asdet_currency_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/asdet_currency_txt"
            android:visibility="invisible"
            android:layout_marginEnd="8dp" />
        <Constraint
            android:id="@+id/asdet_currency_txt"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="@+id/cardView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/asdet_owner_txt"
            android:layout_marginEnd="32dp"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_assetName_txt"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/asdet_currency_txt"
            android:elevation="8dp"
            motion:layout_constraintEnd_toEndOf="@+id/cardView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/asdet_owner_txt"
            android:visibility="invisible"
            android:layout_marginEnd="32dp"
            motion:layout_constraintTop_toBottomOf="@+id/asdet_assetName_txt" />
    </ConstraintSet>
</MotionScene>